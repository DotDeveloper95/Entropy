{
  "description": "Tag Helpers to aid with localizing ASP.NET MVC applications.",
  "version": "1.0.0-*",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/Entropy"
  },
  "compilationOptions": {
    "warningsAsErrors": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Mvc.Localization": "1.0.0-*",
    "Microsoft.AspNetCore.Mvc.Razor": "1.0.0-*",
    "Microsoft.NETCore.Platforms": "1.0.1-*"
  },
  "frameworks": {
    "net451": {},
    "dotnet5.6": {
      "imports": "portable-net451+win8"
    }
  },
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ]
}
